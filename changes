import axios from 'axios';

// Axios instance
const api = axios.create({
  baseURL: 'https://10.191.171.12:5443/PyPortal/',
  timeout: 100000,
  headers: {
    'Content-Type': 'application/json',
  },
});

// ðŸ”¸ Service-wise POST
export const postServiceWiseExp5 = async (time) => {
  try {
    const response = await api.post('EISHome/servicewise_top5_FiveM/EXP/', { time });
    return response;
  } catch (error) {
    console.error('Error fetching Service-wise EXP data:', error);
    throw error;
  }
};

export const postServiceWiseSys5 = async (time) => {
  try {
    const response = await api.post('EISHome/servicewise_top5_FiveM/SYS/', { time });
    return response;
  } catch (error) {
    console.error('Error fetching Service-wise SYS data:', error);
    throw error;
  }
};

